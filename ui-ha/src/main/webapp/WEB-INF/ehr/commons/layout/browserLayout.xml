<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

    <!-- 主布局 -->
    <!-- 项目 -->
    <definition name="browserView" template="/WEB-INF/ehr/commons/jsp/browserLayout.jsp">
        <put-attribute name="header" value="/WEB-INF/ehr/commons/jsp/browserCommonHead.jsp" />
        <put-attribute name="pageCss" expression="/develop/browser/${contentPage}.css" />
        <put-attribute name="contentPage" expression="/develop/browser/${contentPage}.jsp" />
        <put-attribute name="footer" value="/WEB-INF/ehr/commons/jsp/browserCommonFoot.jsp" />
        <put-attribute name="pageJs" expression="/develop/browser/${contentPage}.js" />
    </definition>
    <definition name="browserSimpleView" template="/WEB-INF/ehr/commons/jsp/browserSimpleLayout.jsp">
        <put-attribute name="pageCss" expression="/develop/browser/${contentPage}.css" />
        <put-attribute name="contentPage" expression="/develop/browser/${contentPage}.jsp" />
        <put-attribute name="pageJs" expression="/develop/browser/${contentPage}.js" />
    </definition>
</tiles-definitions>